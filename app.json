{
  "expo": {
    "name": "Expense-Tracker",
    "slug": "Expense-Tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "A toy project to get familiar with React Native. It allows you to track your income and expense.",
    "githubUrl": "https://github.com/ysun62/React-Native-Expense-Tracker"
  }
}
